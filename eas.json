{
  "cli": {
    "version": ">= 16.17.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "apk-standalone": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://quivo-backend-3vhv.onrender.com/api"
      }
    },
    "production-standalone": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://quivo-backend-3vhv.onrender.com/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
